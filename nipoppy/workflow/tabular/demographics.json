{
    "data_sources": {
        "redcap": {
            "api_config": "proc/.redcap.json",
            "default_report": "QPN MoCA-UPDRS-Neuropsy data_Sarah",
            "global_query": "global_records_query"
        },
        "local": {
            "redcap_export": "tabular/assessments/COPN-MRIDataReport2023110_DATA_LABELS_2024-02-19_0851.csv",
            "legacy_DOB": "tabular/legacy_freeze/QPN-DOB-90subjects.csv",
            "MRI_dates": "tabular/MRI_acqdata.csv",
            "Suivi_RPQ": "tabular/demographics/Suivi_RPQ.xlsx",  
            "manifest": "tabular/manifest.csv"
        }
    },

    "variables": {
        "participant_id": {
            "type": "string",
            "sources": {
                "local":{
                    "manifest": "participant_id",
                    "MRI_dates": "participant_id",
                    "redcap_export": "Record ID:",
                    "legacy_DOB": "Record ID",
                    "Suivi_RPQ": "subj_id"
                },
                "redcap": {
                    "default_report": "record_id",
                    "global_query": "record_id"
                }
            }
        },
        "visit": {
            "type": "string",
            "sources": {
                "local":{
                    "manifest":"visit",
                    "MRI_dates":"visit",
                    "redcap_export": "Event Name"
                },
                "redcap": {
                    "default_report": "redcap_event_name",
                    "global_query": "redcap_event_name"
                }
            }
        },
        "dob": {            
            "type": "date",
            "sources": {
                "local": {
                    "redcap_export":"Date of Birth/Date de naissance:",
                    "legacy_DOB":"Date of Birth/Date de naissance"
                }
            }
        },
        "sex": {
                "type": "string",
                "sources": {
                    "local":{
                        "Suivi_RPQ":"Sex (M-F-Trans)",
                        "redcap_export":"1. Gender/Genre:"
                    },
                    "redcap": {
                        "default_report": "gender",
                        "global_query": "gender"
                    }
                }
        },
        "group": {
            "type": "string",
            "sources": {
                "local":{
                    "Suivi_RPQ":"Group",
                    "redcap_export":"Enrolment Group:    Groupe d'inscription:"
                },
                "redcap": {
                    "default_report": "enrolment_group_v2",
                    "global_query": "enrolment_group_v2"
                }
            }
        }
    }
}