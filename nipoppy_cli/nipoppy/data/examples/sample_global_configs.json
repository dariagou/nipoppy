{
    "DATASET_NAME": "<DATASET_NAME>",
    "VISITS": [
        "<VISIT_LABEL>"
    ],
    "SESSIONS": [
        "ses-<SESSION_LABEL>"
    ],
    "CONTAINER_CONFIG": {
        "ARGS": [
            "--cleanenv"
        ]
    },
    "BIDS": [
        {
            "NAME": "heudiconv",
            "VERSION": "0.12.2",
            "STEP": "prepare",
            "CONTAINER": "heudiconv_0.12.2.sif",
            "URI": "docker://nipy/heudiconv:0.12.2",
            "INVOCATION": {
                "overwrite": true
            }
        },
        {
            "NAME": "heudiconv",
            "VERSION": "0.12.2",
            "STEP": "convert",
            "CONTAINER": "heudiconv_0.12.2.sif",
            "URI": "docker://nipy/heudiconv:0.12.2",
            "INVOCATION": {
                "heuristic": "<PATH_TO_HEURISTIC_FILE>",
                "overwrite": true,
                "minmeta": true
            },
            "CONTAINER_CONFIG": {
                "ARGS": [
                    "--bind",
                    "<PATH_TO_HEURISTIC_FILE>"
                ]
            }
        },
        {
            "NAME": "dcm2bids",
            "VERSION": "3.1.0",
            "STEP": "prepare",
            "CONTAINER": "dcm2bids_3.1.0.sif",
            "URI": "docker://unfmontreal/dcm2bids:3.1.0"
        },
        {
            "NAME": "dcm2bids",
            "VERSION": "3.1.0",
            "STEP": "convert",
            "CONTAINER": "dcm2bids_3.1.0.sif",
            "URI": "docker://unfmontreal/dcm2bids:3.1.0",
            "INVOCATION": {
                "config": "<PATH_TO_CONFIG_FILE>"
            },
            "CONTAINER_CONFIG": {
                "ARGS": [
                    "--bind",
                    "<PATH_TO_CONFIG_FILE>"
                ]
            }
        }
    ],
    "PROC_PIPELINES": [
        {
            "NAME": "fmriprep",
            "VERSION": "23.1.3",
            "CONTAINER": "fmriprep_23.1.3.sif",
            "URI": "docker://nipreps/fmriprep:23.1.3",
            "INVOCATION": {
                "fs_license_file": "<PATH_TO_FREESURFER_LICENSE_FILE>",
                "skip_bids_validation": true,
                "regressors_all_comps": true,
                "write_graph": true,
                "notrack": true,
                "nprocs": "8",
                "omp_nthreads": "4",
                "memory_gb": "4G",
                "verbose_count": "-v"
            },
            "CONTAINER_CONFIG": {
                "ARGS": [
                    "--bind",
                    "<PATH_TO_FREESURFER_LICENSE_FILE>"
                ]
            }
        },
        {
            "NAME": "fmriprep",
            "VERSION": "20.2.7",
            "CONTAINER": "fmriprep_20.2.7.sif",
            "URI": "docker://nipreps/fmriprep:20.2.7",
            "INVOCATION": {
                "anat_only": true,
                "fs_license_file": "<PATH_TO_FREESURFER_LICENSE_FILE>",
                "skip_bids_validation": true,
                "regressors_all_comps": true,
                "write_graph": true,
                "notrack": true,
                "nprocs": "8",
                "omp_nthreads": "4",
                "memory_gb": "4G",
                "verbose_count": "-v"
            },
            "CONTAINER_CONFIG": {
                "ARGS": [
                    "--bind",
                    "<PATH_TO_FREESURFER_LICENSE_FILE>"
                ]
            }
        },
        {
            "NAME": "mriqc",
            "VERSION": "23.1.0",
            "CONTAINER": "mriqc_23.1.0.sif",
            "URI": "docker://nipreps/mriqc:23.1.0",
            "INVOCATION": {
                "modalities": [
                    "T1w",
                    "T2w"
                ],
                "no_sub": true,
                "nprocs": "4"
            },
            "TRACKER_CONFIG": {
                "pipeline_complete": [
                    "[[NIPOPPY_BIDS_ID]]/[[NIPOPPY_SESSION]]/anat/[[NIPOPPY_BIDS_ID]]_[[NIPOPPY_SESSION]]_*_T1w.json",
                    "[[NIPOPPY_BIDS_ID]]_[[NIPOPPY_SESSION]]_*_T1w.html"
                ]
            }
        }
    ]
}
